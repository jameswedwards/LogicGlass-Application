 <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.js"></script>

 <script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.7/js/bootstrap.min.js"></script>

 <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.19.1/moment.js"></script>

 <script src="https://drvic10k.github.io/bootstrap-sortable/Scripts/bootstrap-sortable.js"></script>

 <link href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css" rel="stylesheet">
<div class="background">
<p id="notice"><%= notice %></p>

<h1>Sponsors</h1>
<div class ="search_bar">
    <%= form_tag sponsors_path, :method => 'get' do %>
        <%= text_field_tag :search, params[:search] %>
        <%= submit_tag "Search" %>
    <% end %>
    <br>
  </div>
<section class="">
<div class="table104">
  <%= link_to 'New Sponsor', new_sponsor_path, class: "add" %> <%= link_to 'Export Data', '/sponsors.pdf', class: "add" %> <%= link_to 'Back', 'javascript:history.back()', class: "backbtn" %>
<table class="table table-bordered sortable">
  <thead>
    <tr class="table100-head">
      <th>Company</th>
      <th>Sponsor Name</th>
      <th>Primary Phone</th>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sponsors.each do |sponsor| %>
      <tr>
        <td><%= sponsor.company %></td>
        <td><%= sponsor.first_name + ' ' + sponsor.last_name %></td>
        <td><%= sponsor.mobile_phone %></td>
        <td><%= sponsor.sponsor_type.sponsor_type %></td>
        <td class="column6"><%= link_to 'View Contact', sponsor, class: "show" %></td>
        <td class="column7"><%= link_to 'Edit Contact', edit_sponsor_path(sponsor), class: "show" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

</div>
</section>
</div>
