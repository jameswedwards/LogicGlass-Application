<div class="background">
<p id="notice"><%= notice %></p>

<h1>Sponsors</h1><br>

<section class="">
<div class="table104">
<table>
  <thead>
    <tr class="table100-head">
      <th class="column1">Company</th>
      <th class="column2">Sponsor name</th>
      <th class="column3">Primary phone</th>
      <th class="column5">Sponsor type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sponsors.each do |sponsor| %>
      <tr>
        <td class="column1"><%= sponsor.company %></td>
        <td class="column2"><%= sponsor.first_name + ' ' + sponsor.last_name %></td>
        <td class="column3"><%= sponsor.mobile_phone %></td>
        <td class="column5"><%= sponsor.sponsor_type.sponsor_type %></td>
        <td class="column6"><%= link_to 'View Contact', sponsor, class: "show" %></td>
        <td class="column7"><%= link_to 'Edit Contact', edit_sponsor_path(sponsor), class: "show" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to 'New Sponsor', new_sponsor_path, class: "add" %>
<%= link_to 'Export Data', '/sponsors.pdf', class: "add" %>
</div>
</div>
</section>
