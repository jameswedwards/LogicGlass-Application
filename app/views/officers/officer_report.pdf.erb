<h1>Reports powered by LogicGlass</h1>
    <table class="table">
        <thead>
            <tr>
            <th style="width: 200px;">PeopleSoft Number</th>
                <th style="width: 200px;">First Name</th>
                <th style="width: 200px;">Last Name</th>
                <th style="width: 200px;">Position</th>
                <th style="width: 200px;">Student Class</th>
                <th style="width: 200px;">Student Major</th>
                <th style="width: 200px;">Payment Status</th>
                <th style="width: 200px;">Officer Status</th>
            </tr>
        </thead>
        <tbody>
        <% @officers.each do |officer| %>
            <tr>
                <td><%= officer.peoplesoft_id %></td>
                <td><%= officer.first_name %></td>
                <td><%= officer.last_name %></td>
                <td><%= officer.officer_position.position%></td>
                <td><%= officer.student_class.student_class%></td>
                <td><%= officer.major%></td>
                <td><%= officer.payment%></td>
                <td><%= officer.officer_status.status %></td>
            </tr>
        <% end%>
        </tbody>
    </table>
    

